public class Fibonacci {
    public static void main(String[] args) {
        int n = 10; // Number of terms
        System.out.println("Fibonacci Series (Iterative):");
        fibonacciIterative(n);
    }
 
    public static void fibonacciIterative(int n) {
        int a = 0, b = 1;
        for (int i = 0; i < n; i++) {
            System.out.print(a + " ");
            int next = a + b;
            a = b;
            b = next;
        }
    }
}
